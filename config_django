DATABASES = {
        'default': {
            'ENGINE': 'django.db.backends.mysql',
            'NAME': os.getenv('MYSQL_DATABASE'),
            'USER': os.getenv('MYSQL_USER'),
            'PASSWORD': os.getenv('MYSQL_PASSWORD'),
            'HOST': os.getenv('HOST'),
        }
    }
SECRET_KEY = os.getenv('SECRET_KEY')
DEBUG = os.getenv('DEBUG')